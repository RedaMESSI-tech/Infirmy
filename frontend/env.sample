# Configuration de l'API
VITE_API_BASE_URL=http://localhost:3000/api
VITE_API_TIMEOUT=10000

# Configuration de la carte
VITE_MAP_TILE_URL=https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png
VITE_NOMINATIM_URL=https://nominatim.openstreetmap.org
VITE_PHOTON_URL=https://photon.komoot.io

# Configuration de l'upload
VITE_UPLOAD_MAX_SIZE=5242880
VITE_UPLOAD_ALLOWED_TYPES=image/jpeg,image/png,image/webp

# Configuration de l'authentification
VITE_JWT_STORAGE_KEY=infirmy_token
VITE_REFRESH_TOKEN_KEY=infirmy_refresh_token

# Configuration des notifications
VITE_PUSH_PUBLIC_KEY=your_push_public_key_here

# Configuration des tests
VITE_TEST_MODE=false
VITE_MOCK_API=false

# Configuration de l'analytics
VITE_GA_TRACKING_ID=your_ga_tracking_id_here

# Configuration de l'environnement
VITE_APP_ENV=development
VITE_APP_VERSION=1.4.12
